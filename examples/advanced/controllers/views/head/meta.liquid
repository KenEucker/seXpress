{% assign pageMeta = page.meta || undefined %}
{{ pageMeta | json | console_log: nonce }}

<title>{{ pageMeta.title }}</title>
<meta charset="{{ pageMeta.charset || "utf-8" }}" />

<meta http-equiv="X-UA-Compatible" content="IE=9;IE=10;IE=Edge,chrome=1" />
<meta http-equiv="Content-Type" content="{{ pageMeta.contentType || "text/html; charset=UTF-8" }}" />

{% if pageMeta %}
	{{ "viewport" | meta_name: meta.charset || "width=device-width, initial-scale=1, user-scalable=no" }}
	{{ "og:type" | meta_prop: meta.type }}
	{{ "og:url" | meta_prop: meta.url}}
	{{ "og:image" | meta_prop: meta.image}}
	{{ "og:title" | meta_prop: meta.title}}
	{{ "og:description" | meta_prop: meta.description}}
{% endif %}
